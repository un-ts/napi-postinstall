{
  "name": "native-postinstall",
  "version": "0.0.0",
  "type": "module",
  "description": "The `postinstall` script helper for handling native bindings in legacy `npm` versions",
  "repository": "git+https://github.com/un-ts/native-postinstall.git",
  "author": "JounQin <admin@1stg.me> (https://www.1stG.me)",
  "funding": "https://opencollective.com/unts",
  "license": "MIT",
  "packageManager": "npm@11.3.0",
  "engines": {
    "node": "^12.20.0 || ^14.18.0 || >=16.0.0"
  },
  "exports": "./src/index.ts",
  "files": [
    "index.d.cts",
    "lib",
    "!**/*.tsbuildinfo"
  ],
  "scripts": {
    "build": "concurrently -r 'npm:build:*'",
    "build:r": "r -f cjs",
    "build:tsc": "tsc -b src",
    "clean": "rimraf -g .type-coverage coverage dist lib '.*cache'",
    "codesandbox:install": "npm i",
    "dev": "vitest",
    "docs": "vite",
    "docs:build": "vite build",
    "format": "prettier --write .",
    "lint": "concurrently -r 'npm:lint:*'",
    "lint:es": "eslint . --cache --max-warnings 10",
    "lint:style": "stylelint . --cache",
    "lint:tsc": "tsc -b --noEmit",
    "prepare": "simple-git-hooks || exit 0",
    "release": "npm run build && clean-pkg-json && changeset publish",
    "serve": "vite preview",
    "test": "vitest run",
    "typecov": "type-coverage",
    "version": "changeset version && npm i"
  },
  "devDependencies": {
    "@1stg/browserslist-config": "^2.1.4",
    "@1stg/common-config": "^13.0.1",
    "@1stg/postcss-config": "^6.0.6",
    "@1stg/stylelint-config": "^6.0.5",
    "@changesets/changelog-github": "^0.5.1",
    "@changesets/cli": "^2.29.2",
    "@commitlint/cli": "^19.8.0",
    "@eslint-react/eslint-plugin": "^1.48.4",
    "@mdx-js/rollup": "^3.1.0",
    "@pkgr/rollup": "^6.0.3",
    "@shikijs/rehype": "^3.2.2",
    "@size-limit/preset-small-lib": "^11.2.0",
    "@types/mdx": "^2.0.13",
    "@types/node": "^22.14.1",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@types/web": "^0.0.220",
    "@vercel/analytics": "^1.5.0",
    "@vitejs/plugin-react-swc": "^3.9.0",
    "@vitest/coverage-v8": "^3.1.1",
    "@vitest/eslint-plugin": "^1.1.43",
    "clean-pkg-json": "^1.2.1",
    "concurrently": "^9.1.2",
    "eslint": "^9.24.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "github-markdown-css": "^5.8.1",
    "nano-staged": "^0.8.0",
    "prettier": "^3.5.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.5.1",
    "rehype-slug": "^6.0.0",
    "remark-gfm": "^4.0.1",
    "rimraf": "^6.0.1",
    "simple-git-hooks": "^2.12.1",
    "size-limit": "^11.2.0",
    "stylelint": "^16.18.0",
    "type-coverage": "^2.29.7",
    "typescript": "^5.8.3",
    "vite": "^6.3.2",
    "vitest": "^3.1.1"
  },
  "resolutions": {
    "prettier": "^3.5.3"
  },
  "publishConfig": {
    "main": "./lib/index.cjs",
    "types": "./index.d.cts",
    "module": "./lib/index.js",
    "exports": {
      ".": {
        "import": {
          "types": "./lib/index.d.ts",
          "default": "./lib/index.js"
        },
        "require": {
          "types": "./index.d.cts",
          "default": "./lib/index.cjs"
        }
      },
      "./package.json": "./package.json"
    }
  },
  "typeCoverage": {
    "atLeast": 100,
    "cache": true,
    "detail": true,
    "ignoreAsAssertion": true,
    "ignoreFiles": [
      "**/*.d.ts"
    ],
    "ignoreNonNullAssertion": true,
    "showRelativePath": true,
    "strict": true,
    "update": true
  }
}
